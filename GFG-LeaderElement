class Solution {
    static ArrayList<Integer> leaders(int arr[]) {
        // code here
        int leader = 0;
        ArrayList<Integer> list = new ArrayList<>();
        for(int i = arr.length-1 ; i>=0; i--){
            if(arr[i]>=leader){
                leader = arr[i];
                list.add(0, leader);
            }
        }
        return list;
    }
}
