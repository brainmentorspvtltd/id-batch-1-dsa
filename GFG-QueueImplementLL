/*
class QueueNode
{
    int data;
    QueueNode next;
    QueueNode(int a)
    {
        data = a;
        next = null;
    }
}
*/

class MyQueue {
    QueueNode front, rear;

    // Function to push an element into the queue.
    void push(int a) {
        // code here
        QueueNode node = QueueNode(a);
        if(front == null){
        front = rear = node;
        }
        else{
            rear.next = node;
            rear = node;
        }
        
    }

    // Function to pop front element from the queue
    int pop() {
        if(front == null){
            return -1;
        }
        int val = front.data;
        front = front.next;
        return val;
        
        // code here
    }
}
